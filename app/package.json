    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-web": "gulp && ng build --configuration=prod --aot --build-optimizer && gulp build-web",
    "build-mobile": "gulp && ng build --base-href . --configuration=prod --aot --output-path \"www\"",
    "initialize-android": "gulp create-www && cordova platform add android",
    "initialize-ios": "gulp create-www && cordova platform add ios",
    "android-emulate": "npm run build-mobile && cordova emulate android",
    "android-build": "npm run build-mobile && cordova build android",
    "ios-emulate": "npm run build-mobile && cordova emulate ios",
    "ios-build": "npm run build-mobile && cordova build ios",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js",
    "build-web-gcp": "gulp && ng build --base-href /web --configuration=prod --aot --build-optimizer && gulp build-web"
  },
